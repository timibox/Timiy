{# NAV 导航部分, 遍历主题配置文件中的 menu 生成 link #}
<div id="cat-nav" class="cat-branding">
	<div class="cat_menu">
		<div class="menu-topnav-container">
			<ul id="cat-menu" class="menu">
				<li class="menu-item common">
					{% if theme.blog_logo %}
						{% if not is_home() %}{% endif %}
					{% else %}
						<a href="{{ config.root }}" title="Home">首 页</a>
					{% endif %}
					<a href="{{ config.root }}" title="Home">首 页</a>
				</li>
				{% if theme.menu %}
					{% for menu,link in theme.menu %}
						<li>
							<a href="{{ link }}" title="{{ menu }}">{{ menu }}</a>
						</li>
					{% endfor %}
				{% endif %}
			</ul>
		</div>
		{% if theme.local_search %}{% endif %}
		<div class="site-nav-right">
			{% if theme.local_search %}{% endif %}
			<div id="header-tool">
				<div class="popover popover-bottom ">
					<a href="javascript:(0);">
						<svg class="icon" viewbox="0 0 1025 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="11710">
